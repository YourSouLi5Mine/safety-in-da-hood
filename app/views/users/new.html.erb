<div class="row text-center">
  <p id="notice"><%= notice %></p>
  
  <h1>Sign Up</h1>
  
  <%= form_for @user do |f| %>
    <%= f.label :username%>
    <%= f.text_field :username %></br>

    <%= f.label :email %>
    <%= f.text_field :email %></br>
    
    <%= f.label :password %>
    <%= f.text_field :password %></br>
  
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %></br>
  
    <%= f.submit %>
  <% end %>
</div>
