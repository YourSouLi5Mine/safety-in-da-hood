<h1 class="text-center">All users</h1>

  <div class="col-md-6 col-md-offset-3">
    <%= will_paginate @users %>
    <ul class="users">
      <% @users.each do |user| %>
        <li>
          <%= gravatar_for user, size: 50 %>
          <%= user.username %>
        </li>
      <% end %>
    </ul>
    <%= will_paginate @users %>
  </div>
</div>
